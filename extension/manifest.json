{
  "manifest_version": 2,

  "name": "FindYou",
  "description": "Search video using image as input",
  "version": "1.1.0",

  "background" : {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon/icon-19.png",
    "default_title": "youfind"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["style.css"],
      "js": ["script.js", "thirdParty/jquery.min.js"]
    }
  ],

  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],

  "web_accessible_resources": [
        "style.css",
        "icon/icon-48.png"
  ],

    "icons" : {
    "48" : "icon/icon-48.png",
    "128" : "icon/icon-128.png"
  }
}
